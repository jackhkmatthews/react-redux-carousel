{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "noUnusedLocals": true,
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ],
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}